{
  "name": "msk",
  "version": "1.0.3",
  "description": "Small library to mask strings",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
