    "string"
  ],
  "author": "Breno Calazans (@breno_calazans)",
  "license": "MIT",
  "repository": "vtex/msk",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^4.1.1",
    "eslint-plugin-jest": "^20.0.3",
    "jest": "^20.0.4",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "uglify-js": "^3.4.3"
